{
    "name": "vuryss/dokky",
    "description": "OpenAPI 3.1 documentation generator for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dokky\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dokky\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^4.2",
        "justinrainbow/json-schema": "^6.6.3",
        "symfony/serializer": "^7.4.2",
        "vuryss/serializer": "^3.0",
        "symfony/var-dumper": "^7.4",
        "phpstan/phpstan": "^2.1.33",
        "captainhook/captainhook": "^5.27.4",
        "friendsofphp/php-cs-fixer": "^3.92",
        "captainhook/hook-installer": "^1.0.4",
        "ramsey/conventional-commits": "^1.6.0",
        "symfony/validator": "^7.4.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "captainhook/hook-installer": true
        }
    },
    "require": {
        "php": "^8.4",
        "symfony/property-info": "^7.4.1",
        "phpstan/phpdoc-parser": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.6.5"
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "coverage": "XDEBUG_MODE=coverage vendor/bin/pest --coverage-html .coverage"
    }
}
